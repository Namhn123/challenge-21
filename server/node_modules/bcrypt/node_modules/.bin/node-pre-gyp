#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Namhn/bootcamp/repos/challenges/challenge-21/server/node_modules/.pnpm/node-pre-gyp@0.14.0/node_modules/node-pre-gyp/bin/node_modules:/mnt/c/Users/Namhn/bootcamp/repos/challenges/challenge-21/server/node_modules/.pnpm/node-pre-gyp@0.14.0/node_modules/node-pre-gyp/node_modules:/mnt/c/Users/Namhn/bootcamp/repos/challenges/challenge-21/server/node_modules/.pnpm/node-pre-gyp@0.14.0/node_modules:/mnt/c/Users/Namhn/bootcamp/repos/challenges/challenge-21/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Namhn/bootcamp/repos/challenges/challenge-21/server/node_modules/.pnpm/node-pre-gyp@0.14.0/node_modules/node-pre-gyp/bin/node_modules:/mnt/c/Users/Namhn/bootcamp/repos/challenges/challenge-21/server/node_modules/.pnpm/node-pre-gyp@0.14.0/node_modules/node-pre-gyp/node_modules:/mnt/c/Users/Namhn/bootcamp/repos/challenges/challenge-21/server/node_modules/.pnpm/node-pre-gyp@0.14.0/node_modules:/mnt/c/Users/Namhn/bootcamp/repos/challenges/challenge-21/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-pre-gyp@0.14.0/node_modules/node-pre-gyp/bin/node-pre-gyp" "$@"
else
  exec node  "$basedir/../../../../../node-pre-gyp@0.14.0/node_modules/node-pre-gyp/bin/node-pre-gyp" "$@"
fi
